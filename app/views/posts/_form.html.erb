
<%= form_for @post, html: { multipart: true } do |f| %>
    <p>
	<%= f.label :title %><br>		
	<%= f.text_field :title %>
	</p>

	<p>
	<%= f.label :body %><br>
	<%= f.text_area :body%>
	</p>
	<p>
	<%= f.label :image %><br>
	<%= f.file_field :image %>
	</p>

	<p>
	 <button type %= f.submit % class="btn btn-primary">Submit</button>
	 </p>
	<% end %>
	

